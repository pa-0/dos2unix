msgstr "Версія MSYS.\n"

#: common.c:316
msgid "Cygwin version.\n"
msgstr "Версія Cygwin.\n"

#: common.c:318
msgid "Windows 64 bit version (MinGW-w64).\n"
msgstr "Windows, 64-бітова версія (MinGW-w64).\n"

#: common.c:320
